
alias all-commands='compgen -c | sort | uniq | less'
alias clip='xclip -se c'
alias cls='clear'
alias cp='cp -p'
alias far='far2l & exit'
alias md='mkdir -p'
alias mic-loop-off='pactl unload-module module-loopback'
alias mic-loop-on='pactl load-module module-loopback latency_msec=1'
alias mkdir='mkdir -p'
alias mpv='mpv -fs -volume=50'
alias myip='curl ifconfig.me'
alias stat="stat -c '%A %a %n'"
alias std='shutdown +0'
alias su='sudo su'
alias sxiv-a='find -name "*" -type f | sort | sxiv -fio'
alias sxiv-at='find -name "*" -type f | sort | sxiv -ftio'
alias sxiv='sxiv -f'
alias x='exit'
alias zathura='zathura --mode=fullscreen'