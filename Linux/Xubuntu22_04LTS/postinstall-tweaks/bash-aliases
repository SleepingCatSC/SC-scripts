
alias mkdir='mkdir -p'
alias md='mkdir -p'
alias cls='clear'
alias std='shutdown now'
alias far='far2l;exit'
alias myip='curl ifconfig.me'
alias x='exit'
alias feh='feh -FZ'
alias feha="find -name '*' | xargs -d '\n' feh -S name -FZ --auto-rotate"
alias feha-jpg="find -name '*.jpg' | xargs -d '\n' feh -S name -FZ --auto-rotate"
alias feha-png="find -name '*.png' | xargs -d '\n' feh -S name -FZ --auto-rotate"
alias stat="stat -c '%A %a %n'"
alias mpv='mpv -fs -volume=50'
alias mic-loop-on='pactl load-module module-loopback latency_msec=1'
alias mic-loop-off='pactl unload-module module-loopback'
alias su='sudo su'